<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite SYSTEM "urlrewrite3.0.dtd">

<urlrewrite>

	<rule>
		<from>^/view/(.*)?(.*)$</from>
		<to type="forward">/view.htm?nodeId=/$1/&amp;$2</to>
	</rule>
	<rule>
		<from>^/edit/(.*)?(.*)$</from>
		<to type="forward">/edit.htm?nodeId=/$1/&amp;$2</to>
	</rule>
    <!--
    <outbound-rule>
        <from>/structr-public-website/view.htm\?nodeId=/(.*)&amp;(.*)</from>
        <to>/$1?$2</to>
    </outbound-rule>
    <outbound-rule>
        <from>/structr-public-website/view/(.*)/(.*)</from>
        <to>/$2</to>
    </outbound-rule>-->

</urlrewrite>